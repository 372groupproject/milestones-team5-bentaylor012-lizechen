global x

procedure main()
    x:= create(1|2|3)
    write(@x)
    write(@x, " number of results ", *x)
    #reset the expression
    x:=^x
    write(@x, " number of results ", *x)
    #example taken out of "the icon programming language" for demonstration
    label:=create("L" || seq())
    write(@label)
    write(@label)
    write(@label)
end